!------------------------------------------------------------------------------
! c o l o r -  s e t t i n g s

*background: #111111
*foreground: #999999

! Default
! 0: black
*color0:  #222222
*color8:  #222222
! 1: red
*color1:  #9f6767
*color9:  #9f6767
! 2: green
*color2:  #92ac68
*color10: #92ac68
! 3: yellow
*color3:  #d0d293
*color11: #d0d293
! 4: blue
*color4:  #9aacc3
*color12: #9aacc3
! 5: magenta
*color5:  #bb77a4
*color13: #bb77a4
! 6: cyan
*color6:  #77bbb5
*color14: #77bbb5
! 7: white
*color7:  #777777
*color15: #777777

!------------------------------------------------------------------------------
! u r x v t

!URxvt*inheritPixmap: true
!URxvt*transparent: true
!URxvg*shading: 190
URxvt*geometry:         112x22
URxvt*font:            xft:DejaVu Sans Mono for Powerline:style=Book:size=12,xft:FontAwesome:size=12
URxvt*urlLauncher:         firefox
URxvt*scrollBar:        false
URxvt*scrollBar_right:        true
URxvt*scrollColor:        #000000
URxvt*letterSpace: -1
URxvt*print-pipe: "cat > /dev/null"

!------------------------------------------------------------------------------
! rofi

rofi*color-enabled: true
rofi*color-window: #111111, #222222, #111111
rofi*color-normal: #111111, #cccccc, #111111, #333333, #cccccc
rofi*color-active: #666666, #ffffff, #666666, #444444, #fdf6e3
rofi*color-urgent: #111111, #d10400, #111111, #8d100d, #fdf6e3

